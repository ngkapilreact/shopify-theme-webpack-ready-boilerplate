{
  "name": "padellife",
  "version": "1.0.0",
  "main": "webpack.mix.js",
  "scripts": {
    "whoami": "shopify whoami",
    "shop": "shopify theme dev --store store-name.myshopify.com",
    "shop-check": "shopify theme check",
    "pull-live": "shopify theme pull -l",
    "pull": "shopify theme pull",
    "dev": "npx mix watch",
    "build": "npx mix --production"
  },
  "private": true,
  "browserslist": {
    "production": [
      "> 1%",
      "last 2 versions",
      "Firefox ESR"
    ],
    "legacyBrowsers": [
      "> 1%",
      "last 2 versions",
      "Firefox ESR"
    ],
    "modernBrowsers": [
      "last 2 Chrome versions",
      "not Chrome < 60",
      "last 2 Safari versions",
      "not Safari < 10.1",
      "last 2 iOS versions",
      "not iOS < 10.3",
      "last 2 Firefox versions",
      "not Firefox < 54",
      "last 2 Edge versions",
      "not Edge < 15"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "laravel-mix": "^6.0.49",
    "resolve-url-loader": "5.0.0",
    "sass": "^1.77.6",
    "sass-loader": "12.1.0",
    "swiper": "^11.1.5"
  },
  "license": "MIT"
}
